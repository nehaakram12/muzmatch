      .formclass{
      margin-left:auto;
      margin-right:auto;
      margin-top: 3%;
      margin-bottom: auto;
      max-width: 400px;
      min-height: 400px;
      padding: 20px 20px 20px 20px;
      font: 16px;
      color: white;
      background-color: #f5f7fd;
      font-family: calibri;
      }
      a{
      text-decoration: none;
      color: lightblue;
      transition: color .3s ease-in-out,border .3s ease-in-out,background .3s ease-in-out,opacity .3s ease-in-out;
      cursor: pointer;
      }
      .inputbox{
      transition: border-color .3s ease-in-out;
      -webkit-appearance: none;
      box-sizing: border-box;
      border: 1px solid #ccd3e7;
      padding: 11px 24px;
      border-radius: 5px;
      outline: 0;
      height: 54px;
      font-size: 16px;
      width: 100%;
      color: #39475a;
      margin-bottom: 10px;
      }
      .modal-box {
      background: #f5f7fd;
      max-width: 450px;
      min-width: 400px;
      padding: 62px 45px 37px;
      margin: 0 auto;
      border-radius: 5px;
      box-shadow: 2px 0 20px rgba(16,22,31,.3);
      text-align: center;
      color: #f5f7fd;
      line-height: 1.5;
      position: relative;
      margin-bottom: 20px;
      }
      .modal-box h3 {
      font-size: 28px;
      font-weight: 500;
      margin-bottom: 10px;
      text-transform: capitalize;
      }
      .modal-box .modalform {
      margin: 0;
      padding: 0;
      border-style: none;
      display: block;
      }
      .text-center {
      text-align: center;
      }
      .ng-hide {
      display: none!important;
      }
      .wrapper {
      padding: 15px;
      }
      .font-bold {
      font-weight: 600;
      }
      .text-red {
      color: #DA2570;
      }
      .modal-box .field {
      margin: auto;
      margin-bottom: 10px;
      }
      .modal-box input {
      transition: border-color .3s ease-in-out;
      -webkit-appearance: none;
      box-sizing: border-box;
      border: 1px solid #ccd3e7;
      padding: 11px 24px;
      border-radius: 5px;
      outline: 0;
      height: 54px;
      font-size: 16px;
      width: 100%;
      color: #39475a;
      line-height: normal;
      font: inherit;
      vertical-align: baseline;
      margin-bottom: 5px;
      }
      .help-block {
      margin-top: -15px;
      margin-left: 5px;
      text-align: left;
      font-size: 15px;
      }
      .modal-box .btn-list {
      padding-bottom: 4px;
      max-width: 360px;
      margin: 0 auto;
      list-style: none;
      }
      .btn {
      transition: color .3s ease-in-out,border .3s ease-in-out,background .3s ease-in-out,opacity .3s ease-in-out;
      padding: 10px 27px;
      font: 700 13px/18px gt_walsheim,"Helvetica Neue",Arial,Helvetica,sans-serif;
      display: inline-block;
      box-shadow: 0 4px 10px rgba(57,71,90,.13);
      /*background: #fff;
      color: #9738f5;*/
      color: #C70039;
      background-color: #ffffff;
      border: 1px solid #C70039;
      cursor: pointer;
      border-radius: 30px;
      letter-spacing: 1px;
      min-width: 130px;
      vertical-align: middle;
      text-transform: uppercase;
      text-align: center;
      }
      .btn:hover{
      /* background: #15baaa;
      color: #fff;*/
      background-color: #C70039;
      color: #ffffff;
      border: 1px solid #ffffff;
      }
      .modal-box ul{
      padding-left: 0px;
      }
      .btn-list li {
      margin-bottom: 19px;
      display: list-item;
      text-align: -webkit-match-parent;
      }
      .btn-list .btn {
      padding: 14px 28px;
      width: 100%;
      }
      .btn-violet {
      background: #C70039;
      color: #fff;
      }
      .btn-violet:hover{
      background: #ffffff;
      color: #C70039;
      border: 1px solid #C70039;
      }
